{
  "name": "Instagram Like Bot",
  "version": "1.0",
  "description": "Automated like bot for Instagram",
  "manifest_version": 2,
  "author": "David Roth",
  "homepage_url": "https://github.com/ItsWasteD/",

  "content_scripts": [{
    "matches": ["http://*.instagram.com/*", "https://*.instagram.com/*"],
    "js": ["js/jquery-3.5.1.min.js", "instagram-crawler.js"]
    }],

  "permissions": ["http://*.instagram.com/*", "https://*.instagram.com/*"]
}
